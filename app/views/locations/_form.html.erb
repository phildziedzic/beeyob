<%= form_for @location do |f| %>
    <div>
      <%= f.label :name %>
      <br />
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :byob %>
      <br />
      <%= f.text_field :byob %>
    </div>
    <div>
      <%= f.label :phone %>
      <br />
      <%= f.text_field :phone %>
    </div>
    <div>
      <%= f.label :address %>
      <br />
      <%= f.text_field :address %>
    </div>
    <div>
      <%= f.label :address2 %>
      <br />
      <%= f.text_field :address2 %>
    </div>
    <div>
      <%= f.label :city %>
      <br />
      <%= f.text_field :city %>
    </div>
    <div>
      <%= f.label :state %>
      <br />
      <%= f.text_field :state %>
    </div>
    <div>
      <%= f.label :zip %>
      <br />
      <%= f.text_field :zip %>
    </div>
  <%= f.submit %>
<% end %>